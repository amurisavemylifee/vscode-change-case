{
  "name": "vscode-change-string-case",
  "version": "0.0.1",
  "displayName": "Change String Case (Quick Fix)",
  "description": "Extension to change string case.",
  "main": "./out/extension.js",
  "author": "Taras Shashurin",
  "publisher": "amurisavemylifee",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/amurisavemylifee/vscode-change-case"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "activationEvents": [
    "onLanguage"
  ],
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "glob": "^8.1.0",
    "typescript": "^5.1.3"
  },
  "categories": [
    "Other",
    "Snippets",
    "Formatters"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-change-case.toCamelCase",
        "title": "Transform to camelCase"
      },
      {
        "command": "vscode-change-case.toPascalCase",
        "title": "Transform to PascalCase"
      },
      {
        "command": "vscode-change-case.toKebabCase",
        "title": "Transform to kebab-case"
      },
      {
        "command": "vscode-change-case.toConstantCase",
        "title": "Transform to CONSTANT_CASE"
      },
      {
        "command": "vscode-change-case.toSnakeCase",
        "title": "Transform to snake_case"
      },
      {
        "command": "vscode-change-case.toLowerCase",
        "title": "Transform to lower case"
      },
      {
        "command": "vscode-change-case.toUpperCase",
        "title": "Transform to UPPER CASE"
      },
      {
        "command": "vscode-change-case.toUpperKebabCase",
        "title": "Transform to UPPER-KEBAB-CASE"
      },
      {
        "command": "vscode-change-case.toDotCase",
        "title": "Transform to dot.case"
      },
      {
        "command": "vscode-change-case.toPathCase",
        "title": "Transform to path/case"
      }
    ],
    "configuration": {
      "title": "Case Change",
      "properties": {
        "case-change.useCamelCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use camelCase in quick fixes."
        },
        "case-change.usePascalCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use PascalCase in quick fixes."
        },
        "case-change.useKebabCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use kebab-case in quick fixes."
        },
        "case-change.useConstantCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use CONSTANT_CASE in quick fixes."
        },
        "case-change.useSnakeCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use snake_case in quick fixes."
        },
        "case-change.useLowerCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use lower case in quick fixes."
        },
        "case-change.useUpperCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use UPPER CASE in quick fixes."
        },
        "case-change.useUpperKebabCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use UPPER-KEBAB-CASE in quick fixes."
        },
        "case-change.useDotCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use dot.case in quick fixes."
        },
        "case-change.usePathCaseInQuickFixes": {
          "type": "boolean",
          "default": true,
          "description": "Determines whether you want to use path/case in quick fixes."
        }
      }
    }
  }
}
